{
    "compilerOptions": {
        "composite": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "removeComments": true,
        "noImplicitAny": true
    },
    "include": ["vite.config.ts"]
}

